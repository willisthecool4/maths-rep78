        const gameContainer = document.getElementById("games");
        games.forEach(game => {
            let div = document.createElement("div");
            div.className = "game";
            div.innerHTML = `<a onclick="openGamePage('${game.name}')" style="cursor:pointer"><img src="${game.img}" alt="${game.name}"><p>${game.name}</p></a>`;
            gameContainer.appendChild(div);
        });
